<Window x:Class="DecisionTree.GUI.IdentificationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="IdentificationWindow" Height="300" Width="300">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="25" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Height="25" Content="Attribut auswählen" HorizontalAlignment="Left">
        </Button>
        <DataGrid Grid.Row="1" ItemsSource="{Binding TableEntryList}" AutoGenerateColumns="False" Name="filteredDataGrid" />
    </Grid>
</Window>
